<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <!-- for dependabot -->
    <PERFLAB_TARGET_FRAMEWORKS Condition="'$(PERFLAB_TARGET_FRAMEWORKS)' == ''">net8.0</PERFLAB_TARGET_FRAMEWORKS>    
    <TargetFrameworks>$(PERFLAB_TARGET_FRAMEWORKS)</TargetFrameworks>
    <TargetFramework Condition="'$(TargetFrameworks)' == ''">net6.0</TargetFramework>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MarkdownLog.NS20" />
    <PackageReference Include="Newtonsoft.Json" />
    <PackageReference Include="BenchmarkDotNet" />
    <PackageReference Include="Perfolizer" />
    <PackageReference Include="System.CommandLine" />
    <PackageReference Include="SharpCompress" />
  </ItemGroup>
</Project>
